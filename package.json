{
  "name": "babel-note",
  "version": "0.0.1",
  "private": true,
  "description": "babel note.",
  "scripts": {
    "use-babel-runtime": "babel ./use-babel-runtime/use-import.js -o ./use-babel-runtime/dist/use-import.js && babel ./use-babel-runtime/not-import.js -o ./use-babel-runtime/dist/not-import.js",
    "use-babel-polyfill": "babel ./use-babel-polyfill/src.js -o ./use-babel-polyfill/dist.js",
    "use-promise-polyfill": "babel ./use-promise-polyfill/src.js -o ./use-promise-polyfill/dist.js",
    "yarn-check": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install, not NPM')\"",
    "dist": "npm run use-babel-runtime && npm run use-babel-polyfill && npm run use-promise-polyfill"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.25.0",
    "promise-polyfill": "^6.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-import": "^v2.7.0"
  },
  "engines": {
    "node": "^6.x"
  }
}
